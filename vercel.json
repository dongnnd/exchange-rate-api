{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node@3.0.0"
        },
        {
            "src": "api/test.js",
            "use": "@vercel/node@3.0.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}